%ol.timeline.clearfix
  %li.spine
    %a{:href => "#", :title => "This is the Spine of Facebook Timline. You can Add Custom Events here"}
  %li.left
    %i.pointer
    .unit
      %ol.actions
        %li.active
          %a{:href => "#", :rel => "Status"}
            %i.icon.icon-status>
            Status
        %li
          %a{:href => "#"}
            %i.icon.icon-photo>
            Photo
        %li
          %a{:href => "#"}
            %i.icon.icon-place>
            Place
        %li
          %a{:href => "#"}
            %i.icon.icon-event>
            Life Event
      / Units
      .actionUnits
        #Status.formUnit
          %i.active
          %textarea{:cols => "30", :id => "", :name => "", :placeholder => "What's on your mind?", :rows => "3"}
          %ol.controls.clearfix
            %li.post
              %button.uibutton.confirm{:type => "submit"} Post
      / / Units
