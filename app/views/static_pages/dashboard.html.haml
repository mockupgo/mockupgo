%h1 Dashboard

%ul.project-list
  = render @projects

.well
  = form_for Project.new, :html => { class: "form-horizontal" } do |f|
    .control-group
      = f.label :name, class: "control-label"
      .controls
        = f.text_field :name
    .control-group
      .controls
        = f.submit class: "btn btn-primary"


